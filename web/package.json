{
  "name": "sublimate-web",
  "version": "0.0.1",
  "dependencies": {
    "@curi/interactions": "^2.0.3",
    "@curi/router": "^2.1.2",
    "@curi/svelte": "^2.0.0-beta.6",
    "@ethersproject/abi": "^5.0.10",
    "@ethersproject/bignumber": "^5.0.13",
    "@ethersproject/bytes": "^5.0.9",
    "@ethersproject/contracts": "^5.0.9",
    "@ethersproject/providers": "^5.0.19",
    "@ethersproject/solidity": "^5.0.8",
    "@hickory/browser": "^2.1.0",
    "@urql/core": "^1.16.2",
    "@urql/devtools": "^2.0.2",
    "graphql": "^15.5.0",
    "named-logs": "0.0.13",
    "named-logs-console": "0.0.14",
    "sublimate-common": "workspace:*",
    "svelte": "^3.32.0",
    "svelte-cryptoicon": "^0.16.1",
    "svelte-hmr": "0.10.2",
    "web3w": "^0.2.39",
    "web3w-torus-loader": "^0.2.4",
    "web3w-walletconnect-loader": "^0.2.0",
    "wonka": "^4.0.15"
  },
  "devDependencies": {
    "@tailwindcss/ui": "^0.5.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/favicons": "^5.5.0",
    "@types/fs-extra": "^9.0.6",
    "@types/node": "^14.14.22",
    "autoprefixer": "^9.8.6",
    "eth-ens-namehash": "^2.0.8",
    "favicons": "^6.2.0",
    "fs-extra": "^9.1.0",
    "ipfs-deploy": "^8.0.1",
    "ipfs-gateway-emulator": "^4.2.1-ipfs.1",
    "postcss": "^7.0.35",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "svelte-check": "^1.1.30",
    "svelte-preprocess": "^4.6.3",
    "svite": "^0.8.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^2.1.0",
    "typescript": "^3.9.7",
    "vite": "^1.0.0-rc.13"
  },
  "scripts": {
    "fix_VITE_ENV": "node _build/fix_VITE_ENV.js",
    "prepare": "npm run generate",
    "validate": "svelte-check && tsc --noEmit",
    "dev": "svite -ts",
    "build": "svite build -ts && npm run spa2static",
    "build:dev": "svite build -ts --mode development && npm run spa2static",
    "serve": "ipfs-emulator --only -d dist -p 8080",
    "build:serve": "npm run build && npm run serve",
    "generate": "ts-node --project _build/tsconfig.json _build/generate.ts",
    "spa2static": "node _build/fixPages.js && ts-node --project _build/tsconfig.json -r tsconfig-paths/register -r svelte/register _build/spa2static.ts",
    "deploy": "ipfs-deploy -p pinata -s sublimate dist"
  }
}
